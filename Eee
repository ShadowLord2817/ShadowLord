end reach(autoset["Sword Size"][3]) end) -- > UI Lib local Material = loadstring(game:HttpGet("https://raw.githubusercontent.com/Kinlei/MaterialLua/master/Module.lua"))() local UI = Material.Load({ Title = "By DEV", Style = 32, SizeX = 400, SizeY = 200, Theme = "Dark" }) local Page = UI.New({ Title = "DEV" }) -- > Char Page.Toggle({ Text = "Coming soon!", Callback = function(value) end, }) Page.Toggle({ Text = "Keep reach when respawn OP", Callback = function(value) autoset["Toggle Reach"] = value end, }) Page.Slider({ Text = "Reach Size", Callback = function(value) autoset["Sword Size"][3] = value reach(value) end, Min = autoset["Sword Size"][1], Max = autoset["Sword Size"][2], Def = autoset["Sword Size"][3] })
